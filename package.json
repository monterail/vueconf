  },
  "engines": {
    "node": ">=0.12.0"
  },
  "//": "CUSTOM CONFIGURATION",
  "config": {
    "//": "Entry files",
    "host": "127.0.0.1",
    "port": "3000",
    "baseUrl": "./../",
    "directories": {
      "source": "src",
      "destination": "build",
      "temporary": "tmp",
      "//": "Directories relative to `source` directory",
      "modules": "_modules",
      "layouts": "_layouts",
      "images": "_images",
      "styles": "_styles",
      "scripts": "_scripts",
      "data": "_data"
    },
    "entries": {
      "js": "*.js",
      "css": "*.{sass,scss}"
    },
    "development": {
      "url": "http://localhost:3000/"
    },
    "staging": {
      "url": "http://monterailcom-v3.herokuapp.com/"
    },
    "production": {
      "url": "http://monterail.com/"
